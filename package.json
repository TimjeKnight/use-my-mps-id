{
  "type": "module",
  "dependencies": {
    "axios": "^1.11.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "node-fetch": "^3.3.2",
    "openai": "^5.10.2"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "@types/node-fetch": "^2.6.12",
    "cpx": "^1.5.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build:server": "tsc -p server/tsconfig.json",
    "build:client": "cd client && npm run build",
    "copy:csv": "cpx 'server/postcode_constituency_files/*.csv' 'dist/postcode_constituency_files/'",
    "copy:prebakedimages": "cpx output/*.png dist/output/",
    "copy:client": "rm -rf dist/client && cp -r client/dist dist/client",
    "full-build": "npm run build:server && npm run build:client && npm run copy:client && npm run copy:csv && npm run copy:prebakedimages",
    "dev": "nodemon"
  }
}
